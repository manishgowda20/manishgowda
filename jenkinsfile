pipeline {
  agents any
    stages {
    stage ('Build') {
      steps {
      sh '''
      sleep 5
      echo "This is a build stage"
      '''
      }
      }
      stage ('Test') {
      steps {
      sh '''
      sleep 5
      echo "This is a Test stage"
      '''
      }
      }
      }
      }
            
                          
                          
        
